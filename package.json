{
  "name": "daily-notes",
  "version": "1.0.0",
  "description": "A modern daily notes application built with Vue 3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .vue,.js,.ts --fix",
    "format": "prettier --write src",
    "type-check": "vue-tsc --noEmit"
  },
  "dependencies": {
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/test-utils": "^2.4.5",
    "autoprefixer": "^10.4.19",
    "daisyui": "^4.10.1",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.24.0",
    "jsdom": "^24.0.0",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.2",
    "vue-tsc": "^2.0.7"
  },
  "keywords": [
    "vue",
    "notes",
    "daily",
    "productivity",
    "journaling",
    "vite",
    "tailwindcss",
    "daisyui"
  ],
  "author": "Martin Egeskov",
  "license": "MIT"
